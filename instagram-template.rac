
<!-- FEED -->
<div class="insta-target pure-g">
  <!-- Loader -->
  {{#instagramData.data:index}}
    <a href={{instagramData.data[index].link}} class="pure-u-1-3 insta-image" onclick='insta.set("lightbox", true); insta.set("current", {{index}})'>
      <img src="{{instagramData.data[index].images.standard_resolution.url}}" alt="" />
    </a>
  {{/instagramData.data}}

</div>